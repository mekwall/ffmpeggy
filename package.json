{
  "name": "ffmpeggy",
  "version": "3.1.3",
  "type": "module",
  "description": "A minimal yet powerful wrapper around ffmpeg and ffprobe",
  "keywords": [
    "ffmpeg",
    "ffprobe",
    "wrapper",
    "stream",
    "transcode",
    "encode",
    "decode"
  ],
  "author": "Marcus Ekwall <marcus.ekwall@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/mekwall/ffmpeggy.git"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "imports": {
    "#/*": "./src/*"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    }
  },
  "files": [
    "dist",
    "CHANGELOG.md",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "rebuild": "yarn build",
    "build:clean": "rimraf dist",
    "check": "tsc --noEmit",
    "check:ci": "tsc --noEmit --pretty",
    "build": "yarn build:clean && yarn check && tsup src/index.ts --dts --format esm,cjs --out-dir dist",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "fmt": "prettier --write .",
    "fmt:check": "prettier --check .",
    "test": "yarn build && vitest run",
    "test:watch": "yarn build && vitest",
    "test:debug": "cross-env DEBUG=ffmpeggy vitest run",
    "test:unit": "vitest run --project=unit",
    "test:integration": "vitest run --project=integration",
    "test:probe": "vitest run --project=probe:integration",
    "test:events": "vitest run --project=events:integration",
    "test:async": "vitest run --project=async:integration",
    "test:multi": "vitest run --project=multi:integration",
    "test:coverage": "yarn build && vitest run --coverage",
    "test:cjs": "vitest run --project=cjs:integration",
    "semantic-release": "semantic-release",
    "semantic-release:dry-run": "cross-env DRY_RUN=true semantic-release --dry-run --no-ci --debug"
  },
  "engines": {
    "node": ">= 18"
  },
  "dependencies": {
    "@esm2cjs/execa": "^6.1.1-cjs.1",
    "debug": "^4.3.2",
    "typed-emitter": "^2.1.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.17.0",
    "@limegrass/eslint-plugin-import-alias": "^1.5.1",
    "@semantic-release/changelog": "^6.0.1",
    "@semantic-release/commit-analyzer": "^13.0.0",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/release-notes-generator": "^14.0.1",
    "@tsconfig/node24": "^24.0.1",
    "@types/debug": "^4.1.7",
    "@types/ffprobe-static": "^2.0.3",
    "@types/node": "^24.0.3",
    "@typescript-eslint/eslint-plugin": "^8.33.1",
    "@typescript-eslint/parser": "^8.33.1",
    "@vitest/coverage-v8": "^3.2.3",
    "cross-env": "^7.0.3",
    "eslint": "^9.17.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-import-resolver-typescript": "^4.4.3",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.5.1",
    "eslint-plugin-unused-imports": "^4.1.4",
    "eslint-plugin-vitest": "^0.5.4",
    "ffmpeg-static": "^5.0.0",
    "ffprobe-static": "^3.0.0",
    "jiti": "^2.4.2",
    "prettier": "^3.6.1",
    "rimraf": "^6.0.1",
    "semantic-release": "^24.2.0",
    "tsup": "^8.5.0",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.33.1",
    "vitest": "^3.2.3"
  },
  "packageManager": "yarn@4.9.2"
}
